<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="ar.com.academia.entities.Socio" table="SOCIO">
		<id name="numeroDeSocio" column="ID" type="java.lang.Integer">
			<generator class="native"/>
		</id>
		<property name="nombreYapellido" column="NOMBRE_APELLIDO" type="java.lang.String"/>
		<property name="documento" column="DOCUMENTO" type="java.lang.String"/>
		<property name="direccion" column="DIRECCION" type="java.lang.String"/>
		<property name="telefono" column="TELEFONO" type="java.lang.Integer"/>
		<property name="mail" column="MAIL" type="java.lang.String"/>
		<property name="fechaDeNacimiento" column="FECHA_DE_NACIMIENTO" type="java.lang.String"/>
		<property name="sexo" column="SEXO" type="java.lang.String"/>
		
	</class>
</hibernate-mapping>